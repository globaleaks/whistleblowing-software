msgstr "Fra: Fløyteblåser"

msgid "Date: {SubmissionDate}"
msgstr "Dato: {SubmissionDate}"

msgid "Label: {TipLabel}"
msgstr "Merkelapp: {TipLabel}"

msgid "Context: {ContextName}"
msgstr "Bindeleddsinformasjon: {ContextName}"

