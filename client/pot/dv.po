msgstr "ކަނޑައެޅިފައިވާ އިންޕުޓް ސައްހައެއް ނޫން:"

msgid "please enter a valid email address."
msgstr "ސައްހަ އީމެއިލް އެޑްރެހެއް ޖައްސަވާށެވެ"

msgid "please enter numbers only."
msgstr "ހަމައެކަނި ނަންބަރު ޖައްސަވާށެވެ"

msgid "Submissions disabled"
msgstr "ސަބްމިޝަން ޑީސްއޭބަލްޑް "

msgid ""
"You are connecting to the server without anonymity and this server supports "
"only anonymous submissions"
msgstr " ތިޔަ ފަރާތުން ސާރވާއަށް ކަނެކްޓްވަނީ ޒާތީ މައުލޫމާތު ސިއްރުވާގޮތަކަށް ނޫން. މި ސާރވާ ސަޕޯޓުކުރަނީ ހަމައެކަނި ކާކުކަން ނޭނގޭ ގޮތައް ހުށައަޅާ ސަބްމިޝަންސް "

msgid "Your report was successful."
msgstr "ރިޕޯޓު ކުރެވިއްޖެ"

msgid "Remember your receipt for this report."
msgstr "މި ރިޕޯޓާއޮި ގުޅޭ ރަސީދު ހަނދާން ބަހައްޓަވާ"

msgid ""
"Use the 16 digit receipt to log in. It will allow you to view any messages "
"we sent you, and also to add extra info."
msgstr "ލޮގްއިން ވުމަށް ލިބިފައިވާ 16 އަކުރުގެ ރަސީދު ބޭނުންކުރައްވާ. މި ރަސީދު ބޭނުން ކޮށްގެން ތިޔަ ފަރާތަށް ލިބިފައިވާ މެސެޖުތަކާއި އިތުރު މައްލޫއްމާތު ފެންނާނެ"

msgid "View your report"
msgstr "ތިޔަ ފަރާތުގެ ރިޕޯޓް ހުޅުވުމަށް"

msgid "Select the recipients of your report"
msgstr ""

msgid "Recipients selected:"
msgstr "ރިސިޕިއަންޓުން ޚިޔާރުކުރެވިއްޖެ"

msgid "You have reached the maximum number of selectable recipients."
msgstr " ރިސިޕިއަންޓުންގެ ގޮތުގައި އެންމެގިނަވެގެން އިޚްތިޔާރު ކުރެވޭނެ ޢަދަދު ހަމަވެއްޖެ"

msgid "You must select at least one recipient."
msgstr " މަދުވެގެން އެކަކު ރިސިޕިއަންޓެއްގެ ގޮތުގައި އިޚްތިޔާރުކުރަންޖެހޭނެ"

msgid ""
"The following recipients will receive your report and could not be "
"deselected:"
msgstr "ތިރީގައިވާ ރިސިޕިއަންޓުންނަށް ތިޔަ ފަރާތުގެ  ރިޕޯޓް ލިބިވަޑައިގަންނަވާނެ އަދި އެފަރާތްތައް ބާތިލެއް ނުކުރެއްވޭނެ"

msgid ""
"In this step the answers to the following questions are either missing or "
"invalid:"
msgstr "މި ސުވާލުތަކުގެ ޖަވާބުތައް ނުހިމެނޭ ނުވަތަ ހިމެނިފައިވާގޮތް ރަނގަޅެއްނޫން"

msgid "Recipient selection"
msgstr "ރިސިޕިއަންޓް ޚިޔާރުކުރައްވާ"

msgid "Waiting for the file(s) to finish uploading."
msgstr "ފައިލް(ތައް) އަޕްލޯޑް ވަންދެން މަޑުކޮއްލައްވާ"

msgid ""
"The following step-by-step procedure will guide you through creating your "
"whistleblowing platform."
msgstr "ވިސްލްބްލޯ ކުރުމަށް ތިޔަ ފަރާތަށް ޚާއްޞަ ޕްލެޓްފޯމް ހެދުމަށް މިވާ އިރުޝާދުތަކާއި އެއް ގޮތަށް އަމަލުކުރައްވާ"

msgid "Please choose a configuration profile:"
msgstr "ކޮންފިގަރޭޝަން ޕްރޮފައިލްއެއް ހިޔާރުކުރައްވާ: "

msgid "Make it possible for this admin to reset user passwords."
msgstr "މި އެޑްމިންއަށް ޔޫސަރ ޕާސްވޯޑު ބަދަލު ކުރެވޭނެ ގޮތް ހެދުމަަށް "

msgid ""
"We advise selecting this option if you would like to protect data from being"
" lost in the situation where recipients lose their passwords. On the other "
"hand, we would not advise using this feature if you want to setup a system "
"where only recipients are able to access submissions."
msgstr "ރިސިޕިއަންޓްގެ ޕާސްވޯޑް ގެއްލިއްޖެ ހާލަތެއްގައި ހުރިހާ ޑޭޓާ އެއް ނުގެއްލޭ ގޮތައް ރައްކާ ކުރެވިފައި ހުރުމަށް އެދޭނަމަ، މި އޮޕްޝަން އިހްތިޔާރުކުރުމަށް އެދެމެވެ. އެެހެންނަމަވެސް،  ސިސްޓަމް ސެޓްއަޕް ކުރަނީ ހަމައެކަނި ރިސިޕިއަންޓުންނަށް ސަބްމިޝަންސް ބެލޭ ގޮތައް ނަމަ، މި އޮޕްޝަން ހިޔާރުނުކުރެއްވުމަށް އެދެމެވެ. "

msgid "Please choose a different username."
msgstr "އެހެން ޔޫސަރ ނަމެއް ބޭނުން ކުރައްވާ"

msgid "I have read and agree to the terms of the license."
msgstr "އަޅުގަނޑު ލައިސަންސްގެ ޝަރުތުތައް  ކިޔައިފިން، އަދި އެއާއި މެދު އަޅުގަނޑުގެ އެެއްވެސް އިއުތިރާޒެއް ނެތް"

msgid "You have completed the platform wizard."
msgstr "ޕްލެޓްފޯމް ވިޒާޑް ތިޔަ ފަރާތުން ތިވަނީ ފުރިހަމަ ކުރައްވާފަ"

msgid "Please summarize your report in a few words."
msgstr "ރިޕޯޓުގެ ކުރު ހުލާސާއެއް ލިޔޭ"

msgid "Describe your report in detail."
msgstr "ރިޕޯޓާއި ގުޅޭ މައުލޫމާތު ތަފްސީލުކޮށް ލިޔޭ"

msgid "Where did the facts happen?"
msgstr "މި ހާދިސާ ހިނގީ  ކޮންތާކު؟ "

msgid "When did the facts happen?"
msgstr "މި ހާދިސާ ހިިނގީ ކޮން އިރަކު؟"

msgid "I'm a victim"
msgstr "އަޅގަނޑަކީ މި ހާދިސާގެ  ވިކްޓިމްއެއް"

msgid "I'm involved in the facts"
msgstr "އަޅުގަނޑަކީ މި ހާދިސާގެ ތެރޭގައި ހިމެނޭ ފަރާތެއް"

msgid "I witnessed the facts in person"
msgstr "މި ހާދިސާ ހިނގީ އަޅުގަނޑުގެ ހާޒިރުގައި"

msgid "I was personally told by a direct witness"
msgstr "އަޅުގަނޑަށް މި ހާދިސާގެ މައުލޫމާތު ލިބުނީ ސީދާ ހެކިވެރިޔަކުގެ ފަރާތުން"

msgid "It is a rumor I heard"
msgstr "މި ހާދިސާއަކީ އަޅުގަނޑަށް އިވުނު ވާހަކައެއް"

msgid "How are you involved in the reported facts?"
msgstr "ރިޕޯޓްކޮށްފައިވާ ހާދިސާއާއި ތިޔަ ފަރާތާއި  އޮތީ ކޮން ގުޅުމެއް؟"

msgid "Do you have evidence to support your report?"
msgstr "ތިޔަ ރިޕޯޓަށް ބާރުދޭ ހެކި އެބަހުރިތަ؟"

msgid "Please attach the evidence to support your report."
msgstr "ތިޔަ  ރިޕޯޓަށް ބާރުދޭ ހެކި އެޓޭޗްކުރޭ "

msgid "Please describe the evidence in detail."
msgstr "ހުށައަޅާފައިވާ ހެކި ތަފްޞީލުކޮށް ބަޔާންކުރޭ"

msgid ""
"A thorough description of the submitted evidence enhances our ability to "
"evaluate claims and investigate. Please take care to reference significant "
"portions of any videos, images or documents submitted."
msgstr "ހުށަހެޅޭ ހެކިތަކުގެ ފުރިހަމަ ތަފްޞީލު ހިމެނުމަކީ ، ދަޢުވާތައް ބަލާ ތަޙުޤީޤުކުރުމުގެ ޤާބިލުކަން އިތުރުކޮށްދޭނެ ކަމެއް. ހުށަހެޅޭ ވީޑިއޯ، ފޮޓޯ ނުވަތަ ލިޔެކިޔުންތަކުގެ މުހިންމު ބައިތަކަށް ރެފަރެންސް ދިނުމަށް ސަމާލުވޭ"

msgid "Have you reported the facts to other organizations and/or individuals?"
msgstr "މި ހާދިސާ އެއްވެސް އެހެން ޖަމްޢިއްޔާއަކަށް އަދި/ނުވަތަ ވަކިވަކި ފަރުދަކަށް ރިޕޯޓް ކުރެއްވިންތޯ؟"

msgid ""
"Please list the organizations and/or individuals you have informed about "
"these facts."
msgstr "މި ހާދިސާއާއި ގުޅޭ މަޢުލޫމާތު ހިއްސާކުރި ޖަމްޢިއްޔާތަކާއި އަދި/ނުވަތަ  ވަކިވަކި ފަރުދުން ލިސްޓުކުރޭ"

msgid ""
"Have these organizations investigated your claims? If so, what was the "
"outcome?"
msgstr "މި ޖަމްޢިއްޔާތަކުން ތިޔަބޭފުޅުންގެ ދަޢުވާތައް ތަޙުޤީޤު ކޮށްފައިއެބަހުރިތޯ؟ އޭގެ ނަތީޖާއަކީ ކޮބައިތޯ؟"

msgid "What is the outcome you want to achieve with our support?"
msgstr "އަޅުގަނޑުމެންގެ ސަޕޯޓާއެކު ތިޔަބޭފުޅުން ހޯއްދަވަން ބޭނުންފުޅުވާ ނަތީޖާއަކީ ކޮބައިތޯ؟"

msgid "Would you like to tell us who you are?"
msgstr ""

msgid "First name"
msgstr "ފުރަތަމަ ނަން"

msgid "Last name"
msgstr "ފަ ހު ނަން "

msgid "Alternative contact method"
msgstr "ގުޅުމަށް އިތުރު ނަމްބަރެއް"

msgid "I prefer to be contacted via this platform only"
msgstr "އެއްވެސް ކަމަކާއި ގުޅޭގޮތުން އަޅުގަނޑާއި ކޮންޓެކްޓް ކުރުމަށް އެދެނީ ހަމައެކަނި މި ޕްލެޓްފޯމް މެދުވެރިކޮށް"

msgid "Other"
msgstr "އެހެނިހެން"

msgid "Specify"
msgstr ""

msgid "Dear {RecipientName},"
msgstr "{RecipientName} އަށް، "

msgid ""
"You're receiving this email because a user account has been created for you "
"on the system: {Site}"
msgstr "މި އީމެއިލްއަކީ ތިޔަ ފަރާތުގެ ޔޫސަރ އެކައުންޓް ހަދާ ނިމިއްޖެކަން އެންގުމަށް ފޮނުވާފައިވާ އީމެއިލްއެއް: {Site}"

msgid "Username: {Username}"
msgstr "ބޭނުންކުރާމީހާގެ ނަން: {Username}"

msgid "Activation link: {Url}"
msgstr "އެކްޓިވޭޓްކުރުމާ ގުޅޭލިންކް {Url}"

msgid ""
"Please click on the activation link to proceed with the account activation "
"and to set your user password."
msgstr "އެކައުންޓް އެކްޓިވޭޓް ކުރުމަށްޓަކައި ލިބިފައިވާ ލިންކްއަށް އޮބާލައްވާފައި ހިޔާކުރައްވާ ޕާސްވޯޑްއެއް ޖައްސަވާ"

msgid ""
"After successful activation you will be able to access the system via the "
"following link: {LoginUrl}"
msgstr "އެކްޓިވޭޝަން ކާމިޔާބުވުމުން {LoginUrl} ލިންކްއިން ސިސްޓަމް ބޭނުންކުރެވޭނެ"

msgid "Kind regards,"
msgstr "އިޙްތިރާމާއެކު ޝުކުރު ދަންނަވަން"

msgid "Account activation"
msgstr "އެކައުންޓް އެކްޓިވޭޓްކުރުން"

msgid "Your whistleblowing platform is now accessible at:"
msgstr "ތިބާގެ ވިސިލްބްލޯވިން ޕްލެޓްފޯމަށް  ވަދެވޭނީ"

msgid "To log in, visit:"
msgstr "ލޮގް އިން ކުރެއްވުމަށް: ޒިރާރަތްކުރައްވާ"

msgid "Users' credentials:"
msgstr "ބޭނުންކުރައްވާ ފަރާތުގެ ކްރެޑެންޝަލްސް"

msgid "The platform will be automatically deleted on:"
msgstr "ޕްލެޓްފޯމު އޮޓޮމެޓިކުން  ޑިލީޓްވާނީ"

msgid "Access instructions"
msgstr "އިރުޝާދުތަކަށް އެކްސެސްވުމަށް"

msgid ""
"The number of activities recently detected appears to be higher than usual."
msgstr "އާންމު މިންވަރަށް ވުރެ އެކްޓިވިޓީސްގެ އަދަދު ކުއްލިގޮތަކަށް ވަރަށް ގިނަވެފައިވާކަން ފާހަގަކުރެވިއްޖެ"

msgid ""
"This could be a sign of an attack (for example, someone flooding your server"
" with bogus information) or just a usage spike due to an increased "
"visibility of your project."
msgstr "މިއީ ސިސްޓަމްއަށް ލިބޭ ހަމަލާއެއްގެ ނިޝާނެއް (މިސާލަކަށް ކޮންމެވެސް ފަރާތަކުން ސާރވާއަށް ނުރަނގަޅު ބައިވަރު މައުލޫމާތު އަޕްލޯޑްކުރަމުން ދިއުން) ނުވަތަ ތިޔަ ފަރާތުގެ ޕްރޮޖެކްޓުގެ ވިސިބިލިޓީ އިތުރުވުމުގެ ސަބަބުން އައި ޔޫސޭޖް ސްޕައިކްއެއް"

msgid "Examine the issue to determine whether it is legitimate or not."
msgstr "މިއީ ޞައްޙަ މައްސަލައެއްކަން ކަށަވަރު ކުރެއްވުމަށް އިތުރަށް ބައްލަވާ"

msgid "The activities with unusual stats are:"
msgstr "އެކްޓިވިޓީތަކުގެ ތެރޭގައި އާދަޔާ ހިލާފުކަމަށް ފާހަގަ ކުރެވޭ ތަފާސް ހިސާބު ހިމެނެނީ"

msgid "Available disk space: {FreeMemory}/{TotalMemory}"
msgstr ""

msgid ""
"The server cannot guarantee that a new report can be stored, so submissions "
"have been disabled."
msgstr "މި ސާރވާގައި އާ ރިޕޯޓެއް ސޭވް ކުރެވޭނެކަން ޔަޤީނެއް ނޫން، އެހެން ވީމާ އިތުރު ސަބްމިޝަންތައް ވަނީ ޑިސްއޭބަލް ކުރެވިފައި"

msgid ""
"Please consider asking your technical support to create more disk space on "
"the server."
msgstr "ސާރވަރގައި ޑިސްކް ސްޕޭސް އިތުރުކުރަން ބޭނުންފުޅުނަމަ ތިޔަފަރާތުން ޓެކްނިކަލް ސަަޕޯޓުގައި އެކަމަށް އެދެވިދާނެ"

msgid ""
"GlobaLeaks technology includes a server status checking component which will"
" alert you in case something requires your attention."
msgstr "ގްލޯބަލް ލީކްސް ޓެކްނޮލޮޖީ ތެރޭގައި ތިޔަ ފަރާތުގެ ސަމާލުކަމަށް ގެނައުމަށް މި ސަރވާގައި ސްޓޭޓަސް ޗެކް ކުރާނެ ކޮމްފޯނެންޓެއް ހިމެނޭ"

msgid ""
"For more information, log in to the Administration interface and look at the"
" \"System Stats\" and \"Anomalies\" sections."
msgstr "އިތުރު މައުލޫމާތު ބޭނުންނަމަ 'އެޑްމިނިސްޓްރޭޝަން އިންޓަފޭސް' އަށް ލޮގް އިން ކުރައްވާފައި 'ސިސްޓަމް ސްޓެޓްސް' އަދި 'އެނޮމަލީސް' ސެކްޝަން ބައްލަވާ"

msgid "Anomaly detected in {NodeName}"
msgstr " އެނޯމަލީ ޑިޓެކްޓެޑް އިން {NodeName}"

msgid ""
"This is an email to inform you that the PGP key of the following users is "
"going to expire or has already expired:"
msgstr "މި އީމެއިލަކީ ތިރީގައިވާ ޔޫސަރތަކުގެ ޕީ.ޖީ.ޕީ ކީގެ  މުއްދަތު ހަމަވާ ދުވަސް ކައިރިވެއްޖެކަން/ހަމަވެއްޖެކަން އެންގުމަށް ފޮނުވިފައިވާ އީމެއިލްއެއް"

msgid ""
"Without a valid PGP key, the system will not be able to send them encrypted "
"notifications."
msgstr "ސައްޙަ ޕީ.ޖީ..ޕީ ކީއަކާއި ނުލާ ސިސްޓަމްއަކަށް އެންކްރިޕްޓްކޮއްފައިވާ ނޮޓިފިކޭޝަން އެއް ނުފޮނުވޭނެ"

msgid "PGP key expiration alert"
msgstr "ޕީ.ޖީ.ޕީ ކީގެ މުއްދަތު ހަމަވާތީ ފޮނުވޭ އެލާރޓް"

msgid "A new whistleblowing site has been registered."
msgstr " އައު ވިސިލްބްލޯވިންގ ސައިޓެއް ވަނީ ރަޖިސްޓަރވެފައި"

msgid "Registration data:"
msgstr "ރަޖިސްޓްރޭޝަން ޑޭޓާ"

msgid "Site: {Url}"
msgstr "ސައިޓް: {Url}"

msgid "Name: {Name}"
msgstr "ނަން:{Name}"

msgid "Email: {Email}"
msgstr "އީމެއިލް: {Email}"

msgid "New whistleblowing site registered"
msgstr "ވިސްލްބްލޯކުރާ އާ ސައިޓެއް ރަޖިސްޓަރީކުރެވިއްޖެ"

msgid ""
"This is a test email sent out from the platform's administrative interface."
msgstr "މިއީ ޕްލެޓްފޯރމްގެ އެޑްމިނިސްޓްރޭޓިވް އިންޓަފޭސްއިން ފޮނުވާފައިވާ ޓެސްޓް އީމެއިލްއެއް"

msgid ""
"Reception of this email indicates that the server was able to authenticate "
"to and interact with the SMTP mail server."
msgstr "މި އީމެއިލް ފޮނުވިފައި މިވަނީ ސާވާރ އޯތެންޓިކޭޓްވެ އެސް.އެމް.ޓީ.ޕީ މެއިލް ސާވާރއާއި އިންޓަރެކްޓްކުރެވިފައިވާތީ"

msgid "Test email"
msgstr "ޓެސްޓް އީމެއިލް"

msgid ""
"This is an email to notify you that a request has been made to change your "
"email address to {NewEmailAddress}."
msgstr "މި އީމެއިލްއަކީ ތިޔަ ފަރާތުގެ އީމެއިލް އެޑްރެސް {NewEmailAddress}އަށް ބަދަލުކުރަމަށް އެދިފައިވާތީ ފޮނުވާފައިވާ އީމެއިލްއެއް. "

msgid "Click the following link to validate this change:"
msgstr "ގެނައި ބަދަލު ވެލިޑޭޓްކުރުމަށް މި ލިންކަށް ފިއްތަވާލައްވާ"

msgid ""
"If you didn't request this change, change your password and contact your "
"system administrator."
msgstr "އެކައުންޓަށް މި ބަދަލު ގެންނަން އެދިފައި ނުވާނަމަ ވަގުތުން ޕާސްވޯޑް ބަދަލުކޮށް ސިސްޓަމް އެޑްމިނިސްޓްރޭޓަރަކަށް އަންގަވާ"

msgid "Email change request"
msgstr "އީމެއިލް ބަދަލުކުރުމަށް އެދުމަށް"

msgid "From: {Author}"
msgstr "ފޮނުވާ ފަރާތް:{Author}"

msgid "Date: {EventTime}"
msgstr "ތާރީޚް {EventTime}"

msgid "From: Whistleblower"
msgstr "ވިސިލްބްލޯވަރގެ ފަރާތުން"

msgid "Date: {SubmissionDate}"
msgstr "ތާރީޚް {SubmissionDate}"

msgid "Label: {TipLabel}"
msgstr "ލޭބަލް {TipLabel}"

msgid "Status: {TipStatus}"
msgstr "ސްޓޭޓަސް: {TipStatus}"

msgid ""
"The HTTPS certificate loaded on the platform is about to, or has already "
"expired."
msgstr "މި ޕްލެޓްފޯމްއަށް ލޯޑުކޮއްފައިވާ އެޗް.ޓީ.ޓީ.ޕީ.އެސް ސެޓިފިކޭޓްގެ ދުވަސްހަމަވާ ތާރީޙް ހަމަވަނީ/ހަމަވެއްޖެ"

msgid "Expiration date: {ExpirationDate}"
msgstr "މުއްދަތު ހަމަވާ ތާރީޙް: {ExpirationDate}"

msgid ""
"Without a valid certificate, the platform will be accessible in a secure way"
" only via Tor."
msgstr "ސައްޙަ ސެޓްފިކެޓަކާއި ނުލާ މި ޕްލެޓްފޯމް ރައްކާތެރިގޮތެއްގައި ބޭނުންކުރެވޭނީ ހަމައެކަނި ޓޯރ ބްރައުޒަރ ބޭނުން ކޮއްގެން. "

msgid "Log in to solve the issue."
msgstr "މައްސަލް ހައްލުކުރެއްވުމަށް ލޮގް އިން ކުރައްވާ"

msgid "Expiration alert for HTTPS certificate"
msgstr "އެޗް.ޓީ.ޓީ.ޕީ.އެސް ސެޓިފިކޭޓްގެ މުއްދަތު ހަމަވާތީ ފޮނުވޭ އެލަރޓް"

msgid ""
"The automatic HTTPS certificate renewal scheduled for today just failed."
msgstr " މިއަދަށް ޝެޑިއުލްކުރެވިފައިވާ އޮޓޯމެޓިކް އެޗް.ޓީ.ޓީ.ޕީ.އެސް ސެޓްފިކެޓް ރިނިއުއަލް ފެއިލްވެއްޖެ"

msgid "The system will keep trying."
msgstr "ސިސްޓަމް އިތުރަށް މަސައްކަތްކުރަމުންދޭ"

msgid "Failed HTTPS certificate renewal"
msgstr "އެޗް.ޓީ.ޓީ.ޕީ.އެސް ސެޓްފިކެޓް އައު ކުރެވޭގޮތެއް ނުވި"

msgid ""
"This is an email to notify you that a custodian has authorized you to access"
" to the whistleblowing identity for the report {TipNum}."
msgstr "މި އީމެއިލްއަކީ {TipNum} ރިޕޯޓްގެ ވިސިލްބްލޯވަރގެ އައިޑެންޓިޓީ ހޯދުމަށް އޮތް އެކްސެސް ތިޔަ ފަރާތަށް ކަސްޓޯޑިއަންއެއް ދީފައިވާކަން އެންގުމަށް ފޮނުވިފައިވާ އީމެއިލްއެއް"

msgid "The report can be accessed at:"
msgstr "މި ރިޕޯޓަށް އެކްސެސްވުމަށް :"

msgid "Access to whistleblower's identity authorized"
msgstr "ވިސިލްބްލޯވަރގެ އައިޑެންޓިޓީއާ ގުޅޭ މައުލޫމާތަށް އެކްސެސް ލިބިއްޖެ"

msgid ""
"This is an email to notify you that a custodian has denied you access to the"
" whistleblowing identity for the report {TipNum}."
msgstr "މި އީމެއިލަކީ  ރިޕޯޓު {TipNum} ގެ ވިސްލްބްލޯވަރގެ އައިޑެންޓިޓީ ހޯދުމަށް އޮތް އެކްސެސް ތިޔަ ފަރާތަށް ދިނުމަށް ކަސްޓޯޑިއަންއެއް އިންކާރުކުރިކަން  އެންގުމަށް ފޮނުވިފައިވާ އީމެއިލެއް "

msgid "Access to whistleblower's identity denied"
msgstr "ވިސިލްބްލޯވަރގެ އައިޑެންޓިޓީއާ ގުޅޭ މައުލޫމާތު ލިބުމުގެ ޙުއްދައެއް ނުލިބޭ"

msgid ""
"This is an email to notify you that a recipient has requested access to the "
"whistleblower's identity for the report {TipNum}"
msgstr "މި އީމެއިލަކީ ރިޕޯޓު {TipNum}  ގެ ވިސިލްބްލޯވަރގެ އައިޑެންޓިޓީ ހޯދަން ރިސިޕިއަންޓެއް އެދިފައިވާކަން އަންގައި ދިނުމަށް ފޮނުވިފައިވާ އީމެއިލްއެއް"

msgid "The request can be accessed at:"
msgstr "އެދިފައިވާ މަޢުލޫމާތު ހޯދޭނީ:"

msgid "New request of access to a whistleblower's identity"
msgstr "ވިސިލްބްލޯވަރގެ އައިޑެންޓިޓީގެ  މައުލޫމާތު ހޯދަން އެދޭ އައު ރިކުއެސްޓް"

msgid ""
"This is an email to notify you that the whistleblower for the report "
"{TipNum} has provided their identity."
msgstr "މި އީމެއިލްއަކީ {TipNum} ރިޕޯޓްގެ ވިސިލްބްލޯވަރ އެފަރާތުގެ އައިޑެންޓިޓީއާއި ގުޅޭ މައުލޫމާތު ހުށަހަޅާފައިވާކަން  އެންގުމަށް ފޮނުވިފައިވާ އީމެއިލްއެއް"

msgid "The whistleblower has provided their identity"
msgstr "ވިސިލްބްލޯވަރގެ އައިޑެންޓިޓީއާއި ގުޅޭ މައުލޫމާތު ވަނީ ހުށަހަޅާފައި"

msgid ""
"You're receiving this email because a password reset has been requested for "
"the account: {Username}"
msgstr "މި އީމެއިލްއަކީ {Username} އެކައުންޓްގެ ޕާސްވޯޑް ރީސެޓްކުރަން އެދިފައިވާތީ ފޮނުވިފައިވާ އީމެއިލްއެއް"

msgid ""
"If you didn't make this request, you may safely ignore and delete this "
"email."
msgstr "މީ ތިޔަފަރާތުން އެދިފައިވާ އެދުމެއްނޫންނަމަ މި އީމެއިލާ ގުޅިގެން އެއްވެސް ފިޔަވަޅެއް  އަޅާކަށް ނުޖެހޭ އަދި މި އީމެއިލް  ފޮހެލާ"

msgid "You can confirm your request by clicking the following link:"
msgstr "ތިޔަ ބޭނުންފުޅުވާ ކަންތައް ޔަގީންކުރެއްވުމަށް މި ލިންކަށް ފިއްތަވާލައްވާ"

msgid "Password reset instructions"
msgstr "ޕާސްވޯޑް ރީސެޓްކުރާނެގޮތުގެ އިރުޝާދު "

msgid ""
"This is an email to inform you that your PGP key is expiring or has already "
"expired."
msgstr "މި އީމެއިލަކީ ތިޔަފަރާތުގެ ޕީ.ޖީ.ޕީ ކީގެ މުއްދަތު ހަމަވާ ދުވަސް ކައިރިވެއްޖެކަން/ހަމަވެއްޖެކަން އަންގަން ފޮނުވިފައިވާ އީމެއިލްއެއް"

msgid ""
"You should extend its validity and update the key present on the platform, "
"or upload a new key."
msgstr "ވެލިޑިޓީ އިތުރުކުރުމަށް މިހާރު ބޭނުންކުރަމުންދާ ކީ ޕްލެޓްފޯމަށް އަޕްޑޭޓްކުރޭ ނުވަތަ އާ ކީ އަޕްލޯޑުކުރޭ"

msgid ""
"Without a valid PGP key, the system won't be able to encrypt data provided "
"to you."
msgstr "ސައްޙަ ޕީ.ޖީ..ޕީ ކީއަކާއި ނުލާ ސިސްޓަމްއަކަށް ތިޔަފަރާތަށް ފެނިގެންދާ މައުލޫމާތު އެންކްރިޕްޓެއް ނުކުރެވޭނެ"

msgid "Click the link to activate the platform:"
msgstr "ޕްލެޓްފޯމު އެކްޓިވޭޓް ކުރެެއްވުމަށް މި ލިންކަށް ފިއްތަވާލައްވާ"

msgid "Activation"
msgstr "އެކްޓިވޭޓްކުރުން "

msgid "A software update is available."
msgstr "ސޮފްޓްވެއަރގެ ފަހުގެ އަޕްޑޭޓެއް އެބައޮތް"

msgid "It is good security practice to keep the platform up to date."
msgstr "ޕްލެޓްފޯމު އަދާހަމަކޮށްފައި ބެހެއްޓުމަކީ ރައްކާތެރިކަމުގެ ގޮތުން ބަލާއިރު ރަނގަޅު އާދައެއް"

msgid ""
"To learn about new features and bug fixes in the new version consult the "
"change log at: {ChangeLogUrl}"
msgstr "އާ ވަރޝަންގައި ހިމެނޭ ބަގް ފިކްސްތަކާއި އާ ފީޗަރތަކުގެ މަޢުލޫމާތު ހޯދުމަށް ޗޭންޖް ލޮގް ހިޔާރުކުރައްވާ {ChangeLogUrl}"

msgid ""
"For instructions on software updates, please refer to the documentation at: "
"{UpdateGuideUrl}"
msgstr "ސޮފްޓްވެއަރ އަޕްޑޭޓްސްއާ ގުޅޭ އިރުޝާދު ހޯއްދެވުމަށް މި ޑޮކިއުމެންޓޭޝަން ބައްލަވާ: {UpdateGuideUrl}"

msgid ""
"This is an email to notify you that a recipient has granted you access to "
"one or more reports."
msgstr "މި އީމެއިލަކީ  ތިޔަ ފަރާތަށް ރިޕޯޓަކަށް ނުވަތަ ރިޕޯޓުތަކަކަށް އެކްސެސް ވެވޭ ގޮތް ރިސިޕިއަންޓެއް ހަދައި  ދީފައިވާކަން އެންގުމަށް ފޮނުވިފައިވާ އީމެއިލެއް "

msgid "New report"
msgstr "އައު ރިޕޯޓް"

